{
  "name": "its__ms_users",
  "version": "0.0.1",
  "description": "MS Users - user management",
  "license": "MIT",
  "author": {
    "name": "Sergey Dunaevskiy",
    "email": "sergey@dunaevskiy.dev",
    "url": "https://dunaevskiy.dev"
  },
  "scripts": {
    "@ABBR": "------------------------------------------------------------------------------------",
    "b": "yarn build",
    "d": "yarn dev",
    "@PROD": "------------------------------------------------------------------------------------",
    "build": "webpack",
    "start": "node ./build/main.js",
    "@DEV": "-------------------------------------------------------------------------------------",
    "build:watch": "webpack --watch",
    "start:watch": "nodemon --watch ./build ./build/main.js",
    "dev": "run-p build:watch start:watch",
    "@DOCKER": "----------------------------------------------------------------------------------",
    "docker:build": "docker build -t its__ms_template .",
    "@TEST": "------------------------------------------------------------------------------------",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "@LINTER": "----------------------------------------------------------------------------------",
    "lint:check": "yarn prettier:check && yarn eslint:check",
    "lint:fix": "yarn prettier:fix && yarn eslint:fix",
    "eslint:check": "eslint --ext .ts,.tsx .",
    "eslint:fix": "eslint --ext .ts,.tsx --fix .",
    "eslint:staged": "eslint --ext .ts,.tsx --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prettier:staged": "prettier --write --ignore-unknown",
    "@OTHER": "-----------------------------------------------------------------------------------",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "3.10.0",
    "amqplib": "^0.8.0",
    "axios": "^0.26.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "joi": "^17.6.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/amqplib": "^0.8.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "encoding": "^0.1.13",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prisma": "^3.10.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
